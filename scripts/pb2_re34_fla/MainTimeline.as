               }
            }
         }
         else if(this.mens[this.MP_myid].hea / this.mens[this.MP_myid].hmax < this.hp_warn_max)
         {
            if(!this.hpwarning)
            {
               this.hpwarning = true;
               if(this.hpbeepch != null)
