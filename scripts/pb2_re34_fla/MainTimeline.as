                  ++this.hurt_i4;
               }
            }
         }
         if(this.mxnds1 + this.mxnds2 + this.mxnds3 == 0 && this.tnds > 10)
         {
            this.stable = false;
            this.unstable = 1;
         }
         if(this.mens[this.MP_myid].hea <= 0)
