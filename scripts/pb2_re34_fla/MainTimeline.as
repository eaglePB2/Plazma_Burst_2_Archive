            scrolling2 = function():void
            {
               sc_scrool.y = scroll_offset_y + mouseY;
               xx = (sc_scrool.y - 98) / (282 - 98) * (-BADGES_TOTAL * 32 + 296);
               if(xx < -BADGES_TOTAL * 32 + 296)
               {
                  xx = -BADGES_TOTAL * 32 + 296;
               }
               if(xx > 0)
               {
                  xx = 0;
               }
