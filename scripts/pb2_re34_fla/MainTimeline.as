            this.sc_scrool.y = 112 + this.xx / (-this.ch_total * 32 + 220) * (225 - 112);
            if(this.yy > 15)
            {
               this.yy = 15;
            }
            if(this.yy < -15)
            {
               this.yy = -15;
            }
            if(this.yy > 1)
            {
               this.yy = this.yy - 1;
            }
            else if(this.yy < -1)
            {
               this.yy += 1;
            }
            else
            {
               this.yy = 0;
            }
         }
         else
         {
            removeEventListener(Event.ENTER_FRAME,this.onEnterFrame_ac2);
